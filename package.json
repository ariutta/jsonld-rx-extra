    "rx-extra": "git://github.com/ariutta/rx-extra.git#117359e7101c328187c648637c38c69aa60751d2"
  },
  "browser": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "colors": "^1.1.2",
    "csv-parse": "^1.0.2",
    "deep-diff": "^0.3.3",
    "hyperquest": "^1.3.0",
    "leven": "^2.0.0",
    "pretty-data": "^0.40.0",
